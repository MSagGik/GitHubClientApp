<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:backgroundTint">@color/turquoise_100</item>
        <item name="android:background">@color/turquoise_100</item>
        <item name="itemTextAppearanceActive">@style/BottomNavigationView</item>
        <item name="itemTextAppearanceInactive">@style/BottomNavigationView</item>
        <item name="itemIconTint">@color/bottom_navigation_menu_color</item>
        <item name="itemTextColor">@color/bottom_navigation_menu_color</item>
        <item name="labelVisibilityMode">labeled</item>
        <item name="itemRippleColor">@null</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
        <item name="menu">@menu/bottom_navigation_menu</item>
    </style>

    <style name="ViewLine">
        <item name="android:background">@color/turquoise_300</item>
    </style>

    <style name="SwitchSettingMaterial">
        <item name="thumbTint">@color/turquoise_800</item>
        <item name="trackTint">@color/turquoise_300</item>
        <item name="android:checked">false</item>
    </style>

    <style name="BaseHeaderOne">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/text_25</item>
    </style>

    <style name="HeaderOne" parent="BaseHeaderOne">
        <item name="android:textColor">@color/turquoise_900</item>
    </style>

    <style name="BaseHeaderTwo">
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:textFontWeight">300</item>
        <item name="android:textSize">@dimen/text_23</item>
    </style>

    <style name="HeaderTwo" parent="BaseHeaderTwo">
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

    <style name="BaseHeaderTree">
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:textFontWeight">300</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <style name="HeaderTree" parent="BaseHeaderTree">
        <item name="android:textColor">@color/turquoise_900</item>
    </style>

    <style name="BaseEditTextSearch">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textFontWeight">300</item>
        <item name="android:textSize">@dimen/text_19</item>
    </style>

    <style name="EditTextSearch" parent="BaseEditTextSearch">
        <item name="android:textColor">@color/turquoise_100</item>
        <item name="android:textColorHint">@color/turquoise_300</item>
    </style>

    <style name="BaseTextAgreement">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textFontWeight">300</item>
        <item name="android:textSize">@dimen/text_21</item>
    </style>

    <style name="TextAgreement" parent="BaseTextAgreement">
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

    <style name="Button" parent="BaseEditTextSearch">
        <item name="android:textColor">@color/turquoise_100</item>
        <item name="android:backgroundTint">@color/turquoise_800</item>
        <item name="backgroundTint">@color/turquoise_800</item>
    </style>

    <style name="BaseNickname">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/text_21</item>
    </style>

    <style name="Nickname" parent="BaseNickname">
        <item name="android:textColor">@color/turquoise_200</item>
    </style>

    <style name="RepositoryName" parent="BaseNickname">
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

    <style name="BaseSubscribers">
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:textFontWeight">300</item>
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="Subscribers" parent="BaseSubscribers">
        <item name="android:textColor">@color/turquoise_200</item>
    </style>

    <style name="RepositoryMetadata" parent="BaseSubscribers">
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

    <style name="BaseLocation">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="Location" parent="BaseLocation">
        <item name="android:textColor">@color/turquoise_200</item>
    </style>

    <style name="RepositoryDescription" parent="BaseLocation">
        <item name="android:fontFamily">@font/roboto_black_italic</item>
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

    <style name="BaseLanguage">
        <item name="android:fontFamily">@font/roboto_bold_italic</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">@dimen/text_16</item>
    </style>

    <style name="Language" parent="BaseLanguage">
        <item name="android:textColor">@color/turquoise_800</item>
    </style>

</resources>